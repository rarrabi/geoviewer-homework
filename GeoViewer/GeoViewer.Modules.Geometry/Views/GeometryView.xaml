<UserControl x:Class="GeoViewer.Modules.Geometry.Views.GeometryView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:mmc="http://metro.mahapps.com/winfx/xaml/controls" xmlns:p="http://www.codeplex.com/prism"
        xmlns:gg="clr-namespace:GeoViewer.Modules.Geometry" xmlns:ggc="clr-namespace:GeoViewer.Modules.Geometry.Controls" xmlns:ggvm="clr-namespace:GeoViewer.Modules.Geometry.ViewModels"
        Name="Geometry"
        d:DesignWidth="960" d:DesignHeight="720" d:DataContext="{d:DesignInstance IsDesignTimeCreatable=True, Type={x:Type ggvm:GeometryViewModel}}" mc:Ignorable="d">
    <Grid Margin="12,12,12,12">
        <ggc:GeometryControl FeatureSet="{Binding FeatureSet}" />
    </Grid>
</UserControl>
